<Window x:Class="ChangePrinter"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="设定" Height="200" Width="500" WindowStartupLocation="CenterScreen" Icon="/Brilliantech.Packaging.UI.WorkStation;component/ImageMedia/包装系统icon.png">
    <Grid>
        <Canvas>
            <ComboBox Canvas.Left="123" Canvas.Top="8" Height="26" Name="ComboBox_printers" Width="350" />
            <Label Canvas.Left="6" Canvas.Top="6" Content="请选择打印机" Height="28" Name="Label1" />
            <Button Canvas.Left="122" Canvas.Top="121" Content="确定" Height="23" Name="ok" Width="75" />
            <Button Canvas.Left="282" Canvas.Top="121" Content="退出" Height="23" Name="exit" Width="75" />
            <Label Canvas.Left="6" Canvas.Top="63" Content="自动关闭的时间" Height="28" Name="Label2" />
            <TextBox Canvas.Left="123" Canvas.Top="63" Height="28" Name="TextBox_sec" Width="74" />
            <Label Canvas.Left="203" Canvas.Top="63" Content="秒" Height="28" Name="Label3" />
        </Canvas>
    </Grid>
</Window>
